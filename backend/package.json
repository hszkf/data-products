{
  "name": "data-products-backend",
  "version": "1.0.0",
  "description": "SQL Query Studio Backend - Hono + Bun",
  "module": "src/index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir=dist --target=bun",
    "test": "bun test",
    "lint": "oxlint -c oxlint.json src/",
    "lint:fix": "oxlint -c oxlint.json src/ --fix",
    "format": "oxfmt src/",
    "format:check": "oxfmt --check src/"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/uuid": "^10.0.0",
    "oxfmt": "^0.23.0",
    "oxlint": "^1.38.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-redshift-data": "^3.957.0",
    "@aws-sdk/client-s3": "^3.957.0",
    "@aws-sdk/lib-storage": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.957.0",
    "@hono/node-server": "^1.19.7",
    "@types/pg": "^8.16.0",
    "chromadb": "^3.1.8",
    "hono": "^4.11.1",
    "mssql": "^12.2.0",
    "openai": "^6.15.0",
    "pg": "^8.16.3",
    "tedious": "^19.2.0",
    "uuid": "^13.0.0",
    "zod": "^4.2.1"
  }
}
